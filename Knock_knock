print ('Knock knock')
input()
import time
import random

def getAnswer(answerNumber):
    if answerNumber == 1:
      print ('Britney Spears')
      input()
      print ('Britney Spears')
      time.sleep(1)
      return 'Ooops I did it again.'
    elif answerNumber == 2:
      print ('Who')
      input()
      return 'Stop owling around!'
    elif answerNumber == 3:
      print ('It\'s actually just "The Doctor".')
      input()
      return'No, you idiot, it\'s just "The Doctor".'
    elif answerNumber == 4:
      print ('I eat mop')
      input()
      print ('...')
      time.sleep(1)
      return 'That\'s pretty gross, dude.'
    elif answerNumber == 5:
      print ('Mike Huckabee')
      input()
      return 'I forget the punchline but your mother\'s a whore'
    elif answerNumber == 6:
      print ('Doris')
      input()
      return 'Door is locked that\'s why I\'m knocking.'
    elif answerNumber == 7:
      print ('Short-term memory loss')
      input()
      return 'Knock knock'
    elif answerNumber == 8:
      print ('Boo!')
      input()
      return 'Awww don\'t cry. It\'s only a joke.'
  
  
r = random.randint(1, 8)
joke = getAnswer(r)
print(joke)
